//https://practice.geeksforgeeks.org/problems/addition-of-two-square-matrices/0
#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int N;
	    cin>>N;
	    long int a[N][N],b[N][N];
	    for(int i=0;i<N;i++)
	    {    
	        for(int j=0;j<N;j++)
	            cin>>a[i][j];
	    }
	    for(int i=0;i<N;i++)
	    {    
	        for(int j=0;j<N;j++)
	            cin>>b[i][j];
	    }
	   for(int i=0;i<N;i++)
	    {    for(int j=0;j<N;j++)
	         {     
	                cout<<a[i][j]+b[i][j]<<" ";
	         }
	    }
	   
	    cout<<endl;
	}
	return 0;
}
