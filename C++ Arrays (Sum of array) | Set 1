//https://practice.geeksforgeeks.org/problems/c-arrays-sum-of-array-set-1/0
#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int N;
	    cin>>N;
	    int A[N],sum=0;
	    for(int i=0;i<N;i++)
	        cin>>A[i];
	   for(int i=0;i<N;i++)
	        sum+=A[i];
	   cout<<sum<<endl;
	}
	return 0;
}
