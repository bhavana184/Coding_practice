// { Driver Code Starts
#include <bits/stdc++.h>
#include <stdio.h>
using namespace std;

void reverseWords(char*);

/* Driver function to test above functions */
int main() {
    int t;
    cin >> t;
    while (t--) {
        char s[10000];
        cin >> s;
        reverseWords(s);
        cout<<endl;
    }
}// } Driver Code Ends


/*Function to reverse words*/
void reverseWords(char *s) {
    stack<char>sstk;
   int slen=0,i=0;
   while(s[i]!='\0'){//finding length of s
       slen++;
       i++;
   }
   for(int i=slen-1;i>=0;i--){
       if(s[i]!='.'){
           sstk.push(s[i]);
       }
       else{
           while(!sstk.empty()){ 
               cout<<sstk.top();//print top element of stack
                sstk.pop();
          }
          cout<<".";
       }
       
   }
   while(!sstk.empty()){
    cout<<sstk.top();
    sstk.pop();
   }
   
    // Your code here
    
}
