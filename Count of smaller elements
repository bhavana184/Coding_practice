//https://practice.geeksforgeeks.org/problems/count-of-smaller-elements/0
#include <iostream>
using namespace std;

int main() {
	//code
		int t;
	cin>>t;
	while(t--){
	    unsigned long long int n,count=0,x;
	    cin>>n;//size of array
	    unsigned long long int a[n];
	    for(int i=0;i<n;i++)
	        cin>>a[i];
	    cin>>x;//given element
	    for(int i=0;i<n;i++){
	        if(a[i]<=x)
	            count++;
	    }
	    cout<<count<<endl;
	}
	return 0;
}
