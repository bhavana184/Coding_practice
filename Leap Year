#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    unsigned long int n;
	    cin>>n;
	    if(n%100==0){
	        if(n%400==0)
	            cout<<"Yes"<<endl;
	        else
	            cout<<"No"<<endl;
	    }
	    else
	         if(n%4==0)
	            cout<<"Yes"<<endl;
	         else
	            cout<<"No"<<endl;
	}
	return 0;
}
