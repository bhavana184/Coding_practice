//https://practice.geeksforgeeks.org/problems/floyds-triangle/0
#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n,count=1;
	    cin>>n;//n lines
	    for(int i=1;i<=n;i++)
	   {
	       for(int j=1;j<=i;j++)
	        {
	              cout<<count<<" ";
	              count++;
	        } 
	        cout<<endl;
	   }
	}
	return 0;
}
