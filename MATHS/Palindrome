//https://practice.geeksforgeeks.org/problems/palindrome/0
#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    unsigned long int n,n1,rev=0;
	    cin>>n;
	    n1=n;
	    while(n1>0){
	        rev=rev*10+(n1%10);
	        n1=n1/10;
	    }
	    if(n==rev)
	       cout<<"Yes"<<endl;
	    else
	        cout<<"No"<<endl;
	}
	return 0;
}
