//https://practice.geeksforgeeks.org/problems/multiply-array/0
#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n,prod=1;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	        cin>>a[i];
	    for(int i=0;i<n;i++) 
	        prod=prod*a[i];
	   cout<<prod<<endl;
	   
	}
	return 0;
}
