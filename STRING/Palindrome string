#include<iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	//do{
	for(int k=1;k<=t;k++)
	{   int l,flag=0;
	 	cin>> l;
	 	char str[100];
	 	cin>>str;
	 	int j=l-1,i=0;
	 	while(i<=j){
        if(str[i] != str[j]){
            flag = 1;
            break;
            }
        else{
              i++;
              j--;  
            }
        }
         if(flag==1)
            cout<<"No"<<endl;
        else
            cout<<"Yes"<<endl;
	}
		
	return 0;
}
