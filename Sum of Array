#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n,sum=0;
	    cin>>n;//size of array
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++)
	        sum+=a[i];
	        
	    cout<<sum<<endl;
	        
	}
	return 0;
}
