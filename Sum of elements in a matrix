//https://practice.geeksforgeeks.org/problems/sum-of-elements-in-a-matrix/0
#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int N,M;
	    cin>>N>>M;
	    int A[N][M],s=0;
	    for(int i=0;i<N;i++)
	        for(int j=0;j<M;j++)
	            cin>>A[i][j];
	   for(int i=0;i<N;i++)
	        for(int j=0;j<M;j++)
	            s+=A[i][j];
	   cout<<s<<endl;
	}
	return 0;
}
