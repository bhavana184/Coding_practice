#include<iostream>
#include<math.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int k=1;k<=t;k++)
	{
	    int n,s=0;
	    cin>>n;
	    int n1=n;//IMP
	    while(n1>0){
	        int d;
	        d=n1%10;
	        n1=n1/10;
	        s=s+(d*d*d);
	    }
	    if(s==n)
	        cout<<"Yes"<<endl;
	       else
	        cout<<"No"<<endl;
	}
	return 0;
}
