//https://practice.geeksforgeeks.org/problems/largest-element-in-array/0
#include <iostream>
using namespace std;

int main() {
	//code
		int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    unsigned long int a[n],max;
	    for(int i=0;i<n;i++)
	        cin>>a[i];
	   max=a[0];
	   for(int i=0;i<n;i++)
	       if(a[i]>max)
	           max=a[i];
	   cout<<max<<endl;
	}
	return 0;
}
